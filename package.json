{
	"name": "osctf-website",
	"version": "1.0.0",
	"description": "OSCTF website",
	"author": "OSCTF",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/OpenSourceCTF/website.git"
	},
	"engines": {
		"node": ">=9.0.0"
	},
	"scripts": {
		"start": "npm run build && docker-compose -p osctf -f docker/production.yml up",
		"start-dev": "docker-compose -p osctf -f docker/development.yml up & npm run build-dev",
		"server": "node --experimental-modules server/",
		"server-dev": "nodemon -L --watch server/ --experimental-modules -e js,mjs,json,pug server/",
		"build": "webpack --define process.env.NODE_ENV=\"production\" --config build/webpack.production.config.js",
		"build-dev": "webpack-dev-server --config build/webpack.development.config.js",
		"build-stats": "webpack --define process.env.NODE_ENV=\"production\" --config build/webpack.production.config.js --json > stats.json",
		"test": "jest --coverage client/tests/"
	},
	"dependencies": {
		"axios": "^0.17.0",
		"babel-polyfill": "^7.0.0-beta.3",
		"bcrypt": "^1.0.3",
		"classnames": "^2.2.5",
		"dotenv": "^4.0.0",
		"history": "^4.7.2",
		"knex": "^0.14.0",
		"koa": "^2.4.1",
		"koa-body": "^2.5.0",
		"koa-helmet": "^3.3.0",
		"koa-mount": "^3.0.0",
		"koa-passport": "^4.0.1",
		"koa-router": "^7.2.1",
		"koa-session": "^5.5.0",
		"koa-static": "^4.0.1",
		"koa-views": "^6.1.1",
		"match-when": "^3.0.0",
		"mobx": "^3.3.1",
		"mobx-react": "^4.3.4",
		"objection": "^0.9.0",
		"passport-local": "^1.0.0",
		"pg": "^7.4.0",
		"prop-types": "^15.6.0",
		"pug": "^2.0.0-rc.4",
		"react": "^16.2.0",
		"react-dnd": "^2.5.4",
		"react-dnd-html5-backend": "^2.5.4",
		"react-dom": "^16.2.0",
		"react-helmet": "^5.2.0",
		"react-router-dom": "^4.2.2",
		"sanitize.css": "^5.0.0"
	},
	"devDependencies": {
		"autoprefixer": "^7.1.6",
		"babel-core": "^7.0.0-beta.3",
		"babel-eslint": "^8.0.2",
		"babel-jest": "^21.3.0-beta.13",
		"babel-loader": "^7.1.2",
		"babel-plugin-syntax-dynamic-import": "^7.0.0-beta.3",
		"babel-plugin-transform-async-to-generator": "^7.0.0-beta.3",
		"babel-plugin-transform-class-properties": "^7.0.0-beta.3",
		"babel-plugin-transform-decorators-legacy": "^1.3.4",
		"babel-plugin-transform-es2015-modules-commonjs": "^7.0.0-beta.3",
		"babel-plugin-transform-export-default": "^7.0.0-beta.3",
		"babel-plugin-transform-object-rest-spread": "^7.0.0-beta.3",
		"babel-plugin-transform-runtime": "^7.0.0-beta.3",
		"babel-preset-babili": "^0.1.4",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react": "^7.0.0-beta.3",
		"case-sensitive-paths-webpack-plugin": "^2.1.1",
		"css-loader": "^0.28.7",
		"csso-loader": "^0.3.1",
		"eslint": "^4.10.0",
		"eslint-config-standard": "^10.2.1",
		"eslint-import-resolver-webpack": "^0.8.3",
		"eslint-loader": "^1.9.0",
		"eslint-plugin-import": "^2.8.0",
		"eslint-plugin-node": "^5.2.1",
		"eslint-plugin-promise": "^3.6.0",
		"eslint-plugin-react": "^7.4.0",
		"eslint-plugin-security": "^1.4.0",
		"eslint-plugin-standard": "^3.0.1",
		"extract-text-webpack-plugin": "^3.0.2",
		"jest": "^21.2.1",
		"lodash.merge": "^4.6.0",
		"node-sass": "^4.6.0",
		"nodemon": "^1.12.1",
		"postcss-loader": "^2.0.8",
		"react-hot-loader": "^3.1.2",
		"sass-loader": "^6.0.6",
		"style-loader": "^0.19.0",
		"webpack": "^3.8.1",
		"webpack-dev-server": "^2.9.4"
	},
	"jest": {
		"moduleNameMapper": {
			"^Components(.*)$": "<rootDir>/client/components$1",
			"^Modules(.*)$": "<rootDir>/client/modules$1",
			"^API(.*)$": "<rootDir>/client/api$1",
			"^BrowserHistory(.*)$": "<rootDir>/client/browser-history$1"
		}
	}
}
